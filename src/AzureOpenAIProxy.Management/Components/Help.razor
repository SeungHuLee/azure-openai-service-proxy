<MudStack Row="true">
    <MudText Typo="Typo.h6">@Title</MudText>
    <MudSpacer />
    <MudLink Target="_blank" Href="@Link">
        <MudIcon Icon="@Icons.Material.Filled.Help" />
    </MudLink>
</MudStack>

@code {
    [Parameter]
    public string Title { get; set; } = "";

    [Parameter]
    public string Link { get; set; } = "";
}
